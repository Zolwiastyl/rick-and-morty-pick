		"@types/react": "^16.9.41",
		"@types/react-dom": "^16.9.6",
		"@types/react-onclickoutside": "^6.7.3",
		"auth0-js": "^9.13.2",
		"autoprefixer": "^9.8.0",
		"cross-env": "^7.0.2",
		"cypress": "4.10.0",
		"eslint-plugin-cypress": "^2.11.1",
		"eslint-plugin-react-hooks": "^4.0.6",
		"eslint-plugin-simple-import-sort": "^5.0.3",
		"postcss-cli": "^7.1.1",
		"react-test-renderer": "^16.13.1",
		"ts-jest": "^26.1.1",
		"ts-loader": "^7.0.5"
	}

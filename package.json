	"scripts": {
		"get-dark-cli": "curl https://dark-cli.storage.googleapis.com/latest/dark-cli-linux --output dark-cli-linux && chmod +x dark-cli-linux",
		"start": "react-scripts start && tailwind:css --watch",
		"tailwind:css": "postcss ./src/tailwind.css -c tailwind.config.js -o src/index.css --watch",
		"tailwind:build": "postcss src/tailwind.css -o src/index.css",
		"build": "npm run tailwind:build && react-scripts build",
		"build-dark-assets": "PUBLIC_URL=DARK_STATIC_ASSETS_BASE_URL || react-scripts build",
		"choose-canvas": "scripts/choose-canvas.js",
		"build-for": "run-s \"choose-canvas {1}\" build-dark-assets -- $*",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
